{
  "name": "reveal.js",
  "version": "4.3.4",
  "description": "The HTML Presentation Framework",
  "homepage": "https://revealjs.com",
  "subdomain": "revealjs",
  "main": "dist/reveal.js",
  "module": "dist/reveal.esm.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "test": "gulp test",
    "start": "gulp serve",
    "build": "gulp build"
  },
  "author": {
    "name": "Hakim El Hattab",
    "email": "hakim.elhattab@gmail.com",
    "web": "https://hakim.se"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hakimel/reveal.js.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "reveal",
    "slides",
    "presentation"
  ],
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/eslint-parser": "^7",
    "@babel/preset-env": "^7",
    "@dlguswo333/reveal-multiplex": "^0",
    "@rollup/plugin-babel": "^6",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0",
    "babel-plugin-transform-html-import-to-string": "^2",
    "core-js": "^3",
    "fitty": "^2",
    "glob": "^11.0.0",
    "gulp": "^5",
    "gulp-autoprefixer": "^9",
    "gulp-clean-css": "^4",
    "gulp-connect": "^5",
    "gulp-eslint": "^6",
    "gulp-header": "^2",
    "gulp-zip": "^6.0.0",
    "highlight.js": "^11",
    "marked": "^15.0.0",
    "node-qunit-puppeteer": "^2",
    "qunit": "^2",
    "rollup": "^4",
    "sass": "^1",
    "yargs": "^17"
  },
  "browserslist": "> 0.5%, IE 11, not dead",
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": true,
      "requireConfigFile": false
    },
    "globals": {
      "module": false,
      "console": false,
      "unescape": false,
      "define": false,
      "exports": false
    },
    "rules": {
      "curly": 0,
      "eqeqeq": 2,
      "wrap-iife": [
        2,
        "any"
      ],
      "no-use-before-define": [
        2,
        {
          "functions": false
        }
      ],
      "new-cap": 2,
      "no-caller": 2,
      "dot-notation": 0,
      "no-eq-null": 2,
      "no-unused-expressions": 0
    }
  },
  "overrides": {
    "node-qunit-puppeteer": {
      "puppeteer": "24.4.0"
    },
    "lodash.template": {
      "lodash": "4.17.21"
    },
    "gulp-eslint": {
      "eslint": "9.22.0"
    }
  }
}